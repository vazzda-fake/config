escape M-Tab

unmanage panel
#set border 0
#set padding 0 16 0 0

set winfmt %a%n

#exec xmodmap  -e 'keycode 122 = F20'
#exec xmodmap  -e 'keycode 123 = F21'

#system hotkeys
bind grave exec lxsession-logout

bind XF86AudioMute exec amixer set Master toggle
bind XF86AudioLowerVolume exec amixer -c 0 set Master 5- unmute
bind XF86AudioRaiseVolume exec amixer -c 0 set Master 5+ unmute

#progmramm hotkeys
bind F1 exec google-chrome
bind F2 exec x-terminal-emulator
bind F3 exec gvim -S /home/vazzda/.vim/session.vim
bind F4 exec ~/work/tortoisehg-2.7/thg
bind F7 exec teamviewer
bind F11 exec audacious
bind F10 exec alarmclock

bind C-1 number 1
bind C-2 number 2
bind C-3 number 3
bind C-4 number 4
bind C-5 number 5
bind C-6 number 6
bind C-7 number 7
bind C-8 number 8
bind C-9 number 9
